extends success
block title
  title="WestSide App"
  link(rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:400:700")
  link(rel="stylesheet" type="text/css" href="css/client.css")
body
block content
  div(id="ws-header")
    div(id="ws-logo")
      div(id="ws-logo-sb")="W"
  div(id="ws-container")
    div(id="ws-songs")
      each fav in favs
        div(class="ws-song-row")
          div(class="ws-song-avatar")
            img(src=fav.artwork_url||fav.user.avatar_url)
          div(class="ws-song-info")
            ul
              li(class="ws-song-title")=fav.title
              li(class="ws-song-artist")=fav.user.username
              
        div(class="ws-divider")
  script(src="js/socket.io/socket.io.js")
  script(src="js/client.js")
  